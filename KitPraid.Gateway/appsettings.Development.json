{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "identity-route": {
        "ClusterId": "identity-cluster",
        "Match": { "Path": "/identity/{**catch-all}" }
      },
      "identity-ui-route": {
        "ClusterId": "identity-ui-cluster",
        "Match": { "Path": "/identity-ui/{**catch-all}" }
      },
      "product-route": {
        "ClusterId": "product-cluster",
        "Match": { "Path": "/products/{**catch-all}" }
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "d1": { "Address": "https://localhost:5001" },
          "d2": { "Address": "https://localhost:5011" }
        }
      },
      "identity-ui-cluster": {
        "Destinations": {
          "d1": { "Address": "https://localhost:7070" },
          "d2": { "Address": "https://localhost:7072" }
        }
      },
      "product-cluster": {
        "Destinations": {
          "d1": { "Address": "https://localhost:7037" },
          "d2": { "Address": "https://localhost:6162" }
        }
      }
    }
  }
}
